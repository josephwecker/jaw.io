!!! 5
%html(lang="en")
  %head
    %meta(charset="utf-8")
    %meta(http-equiv="content-type" content="text/html; charset=utf-8")
    %meta(content="IE=edge,chrome=1" http-equiv="X-UA-Compatible")
    %title {{page.title}}
    %link(rel="stylesheet" type="text/css" href="/css/a.css")
    %link(rel="shortcut icon" href="favicon.png")
    /[if lt IE 9]
      %script(type="text/javascript" src="http://html5shim.googlecode.com/svn/trunk/html5.js")
    %script(type="text/javascript" src="/js/foutbgone.js")
    %script(type="text/javascript" src="/js/jquery-1.6.4.min.js")
    %script(type="text/javascript" src="/js/auto_align.js")
  %body
    %header
      %hgroup
        %h1
          Joseph A. Wecker
          %span.subtitle I/O
        %h2.page-title {{page.title}}
      - (1..19).each do |i|
        .block(class="c#{i}")= (i - 1)
    %section#main
      - (1..19).each do |i|
        .block2(class="d#{i}")= (i - 1)
      {{content}}
      .clear
    .clear
    %footer
